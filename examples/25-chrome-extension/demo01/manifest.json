{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggestion_key": {
        "default": "Crtl + Shift + F",
        "mac": "MacCrtl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
